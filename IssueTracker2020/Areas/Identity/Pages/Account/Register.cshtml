@page
@model RegisterModel

@{
    ViewData["Title"] = "Register";
    Layout = null;
}

<!DOCTYPE html>
<html dir="ltr" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Browser Responsiveness -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Kengaroo Issue Tracking 2020">

    <!-- Website Title -->
    <title>Register New Account - Kengaroo IT</title>

    <!-- Favicon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/assets/images/newfavicon.png">

    <!-- ==============================================================-->
    <!-- All CSS Files Go Below This Comment -->
    <!-- ==============================================================-->
    <link href="~/dist/css/style.min.css" rel="stylesheet" />
    <!-- ==============================================================-->
    <!-- All CSS Files Go Above This Comment -->
    <!-- ==============================================================-->
    <style>
        .bg {
            /* The image used */
            background-image: url(/assets/images/cover.jpg);
            /* Full height */
            height: 100%;
            /* Center and scale the image nicely */
            background-position: center;
            background-repeat: no-repeat;
            background-size: cover;
        }
    </style>
</head>

<body>
    <div class="main-wrapper">
        <!-- ============================================================== -->
        <!-- Preloader - style you can find in spinners.css -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Preloader - style you can find in spinners.css -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Login box.scss -->
        <!-- ============================================================== -->
        <div class="auth-wrapper d-flex no-block justify-content-center align-items-center bg">
            <div class="auth-box on-sidebar p-4 bg-white m-0">
                <div id="loginform">
                    <div class="logo text-center">
                        <span class="db">
                            <img src="/assets/images/mainlayoutlogo.png" alt="logo" style="width: 300px;" /><br />
                        </span>
                    </div>
                    <!-- Form -->
                    <div class="row">
                        <div class="col-12">
                            <form class="form-horizontal mt-3 form-material" id="loginform" method="post">
                                <div class="form-group mb-3">
                                    <div class="col-xs-12">
                                        <input asp-for="Input.FirstName" class="form-control" type="text" required="" placeholder="First Name">
                                        <span asp-validation-for="Input.FirstName" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group mb-3">
                                    <div class="col-xs-12">
                                        <input asp-for="Input.LastName" class="form-control" type="text" required="" placeholder="Last Name">
                                        <span asp-validation-for="Input.LastName" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group mb-3 ">
                                    <div class="col-xs-12">
                                        <input asp-for="Input.Email" class="form-control" type="text" required="" placeholder="Email">
                                        <span asp-validation-for="Input.Email" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group mb-3 ">
                                    <div class="col-xs-12">
                                        <input asp-for="Input.Password" class="form-control" type="password" required="" placeholder="Password">
                                        <span asp-validation-for="Input.Password" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group mb-3">
                                    <div class="col-xs-12">
                                        <input asp-for="Input.ConfirmPassword" class="form-control" type="password" required="" placeholder="Confirm Password" />
                                        <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group text-center mb-3">
                                    <div class="col-xs-12">
                                        <button class="btn btn-info btn-block text-uppercase waves-effect waves-light" type="submit">Sign Up</button>
                                    </div>
                                </div>
                                <div class="form-group mb-0">
                                    <div class="col-sm-12 text-center">
                                        <p>Already have an account? <a asp-area="Identity" asp-page="/Account/Login" class="text-info ml-1 ">Sign In</a></p>
                                    </div>
                                    <div class="col-sm-12 text-center">
                                        <p>Want to go back <a asp-controller="Home" asp-action="LandingPage" class="text-info ml-1 ">Home?</a></p>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- Login box.scss -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper scss in scafholding.scss -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper scss in scafholding.scss -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Right Sidebar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Right Sidebar -->
        <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- All Required js -->
    <!-- ============================================================== -->
    <script src="~/assets/libs/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="~/assets/libs/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/assets/libs/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- ============================================================== -->
    <!-- This page plugin js -->
    <!-- ============================================================== -->
</body>
</html>







